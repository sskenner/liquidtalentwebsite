    -moz-box-sizing: border-box;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
}

article, aside, details, figcaption, figure, footer, header, hgroup, menu, nav, section { display: block; }
ol, ul { list-style: none; }
blockquote, q { quotes: none; }
blockquote:before, blockquote:after, q:before, q:after { content: none; }

input, textarea {
    outline: none;
    -moz-appearance: none;
    -webkit-appearance: none;
    appearance: none;
    border: 0;
    font-family: 'main', Helvetica, sans-serif;
}
::-webkit-input-placeholder { color: #000; }
:-moz-placeholder { color: #000; }
::-moz-placeholder { color: #000; }
:-ms-input-placeholder { color: #000; }


table {
    border-collapse: collapse;
    border-spacing: 0;
}

a { cursor: pointer; }

body {
    line-height: 1;
    background: url(<%= asset_path 'new/main/background.png' %>);
    color: #000;
    font-family: 'main', Helvetica, sans-serif;
    font-weight: 100;
    font-size: 14px;
    min-width: 660px;
}

a {
    text-decoration: none;
    cursor: pointer;
    color: #239060;
}
a:hover { color: #000; }

.t {
    -webkit-transition: all 0.5s;
    -moz-transition: all 0.5s;
    -o-transition: all 0.5s;
    -ms-transition: all 0.5s;
    -webkit-transition: all 0.5s;
}

#title {
    text-align: center;
    line-height: 60px;
    font-size: 24px;
    font-weight: 400;
    margin-bottom: 20px;
    background: hsla(0, 0%, 100%, 0.6);
    border-bottom: 1px solid #FFF;
    border-right: 1px solid #FFF;
    padding-right: 10px;
}
#title .black-button {
    float: right;
    font-size: 18px;
    height: 40px;
    line-height: 43px;
    padding: 0px 40px;
    font-weight: 100;
    margin-top: 10px;
    margin-left: -40px;
}
#title .green-button {
    font-size: 18px;
    height: 40px;
    line-height: 43px;
    padding: 0px 40px;
    margin: 10px -80px 0px 10px;
    font-weight: 100;
    float:left;
}

.text-field {
    display: block;
    text-indent: 20px;
    background-color: hsla(0, 0%, 100%, 0.3);
    border: 1px solid #FFF;
    outline: 0;
}

.lt-drop {
    width: 21px;
    height: 28px;
    background: url(<%= asset_path 'new/main/drops.png' %>);
}
.lt-drop.hidden { display: none; }
.lt-drop.blue { background-position: 0px 0px; }
.lt-drop.bronze { background-position: -23px 0px; }
.lt-drop.silver { background-position: -45px 0px; }
.lt-drop.gold { background-position: -66px 0px; }

.star {
    vertical-align: top;
    display: inline-block;
    width: 15px;
    height: 14px;
    background: url(<%= asset_path 'main/stars.png' %>);
}
.star.active { background-position: 14px 0px; }

.avatar {
    display: block;
    cursor: pointer;
    border-radius: 10px;
    border: 1px solid transparent;
    box-shadow: -1px 1px 3px 0px #AAA;
}

.green-button {
    display: block;
    background: #239060;
    background-size: 1px 100%;
    color: #FFF;
}
.green-button:hover, .green-button:focus {
    background: #000 !important;
    color: #FFF;
}

.black-button {
    display: block;
    background: #000;
    background-size: 1px 100%;
    color: #FFF;
}
.black-button:hover, .black-button:focus {
    background: #239060 !important;
    color: #FFF;
}

.notification {
    position: absolute;
    right: -10px;
    top: 8px;
    color: #FFF;
    line-height: 25px;
    padding: 3px 9px;
    border-radius: 4px;
    text-indent: 0px;
    background-color: #239060;
    opacity: 0;
}
.notification.active { opacity: 1; }

#wrapper {
    height: 700px;
    width: 960px;
    margin: 0px auto;
    position: relative;
    overflow: hidden;
}
#background {
    position: absolute;
    height: 700px;
    width: 750px;
    right: 0px;
    overflow: hidden;
}
#admin-panel {
    position: relative;
    z-index: 1;
    width: 210px;
    height: 710px;
    background-color: hsla(0, 0%, 100%, 0.3);
    border: 1px solid #FFF;
    bottom: 10px;
}
#admin-panel .header {
    padding: 10px;
    min-height: 180px;
    position: relative;
}

#admin-panel .header .login {
    width: 210px;
    left: 0px;
    bottom: 34%;
    position: absolute;
    text-align: center;
    padding: 14px 18px;
    font-size: 20px;
    border: 6px double #FFF;
    font-weight: 400;
}
#admin-panel .navigation .link {
    display: block;
    width: 190px;
    font-size: 20px;
    line-height: 40px;
    text-indent: 25px;
    margin-bottom: 20px;
    word-spacing: 10px;
    color: #000;
    position: relative;
}
#admin-panel .navigation .link:hover { color: #239060; }
#admin-panel .navigation .link .icon {
    display: inline-block;
    vertical-align: middle;
    width: 29px;
    height: 27px;
    background: url(<%= asset_url 'new/main/icons-navigation.png' %>) repeat-y;
}
#admin-panel .navigation .settings.link .icon.hover,
#admin-panel .navigation .favorites.link .icon.hover,
#admin-panel .navigation .messages.link .icon.hover {
    width: 28px;
    margin-left: 1px;
}
#admin-panel .navigation .search.link .icon.hover,
#admin-panel .navigation .projects.link .icon.hover {
    width: 27px;
    margin-left: 2px;
}
#admin-panel .navigation .search.link .icon { background-position: 0px 0px; }
#admin-panel .navigation .search.link .icon.hover { background-position: -29px 0px; }

#admin-panel .navigation .projects.link .icon {
    height: 25px;
    background-position: 0px -54px;
}
#admin-panel .navigation .projects.link .icon.hover { background-position: -29px -54px; }

#admin-panel .navigation .messages.link .icon { background-position: 0px -79px; }
#admin-panel .navigation .messages.link .icon.hover { background-position: -29px -79px; }

#admin-panel .navigation .favorites.link .icon { background-position: 0px -106px; }
#admin-panel .navigation .favorites.link .icon.hover { background-position: -29px -106px; }

#admin-panel .navigation .stats.link .icon {
    height: 26px;
    background-position: 0px -133px;
}
#admin-panel .navigation .stats.link .icon.hover { background-position: -29px -133px; }

#admin-panel .navigation .settings.link .icon {
    height: 29px;
    background-position: 0px -159px;
}
#admin-panel .navigation .settings.link .icon.hover { background-position: -29px -159px; }

#admin-panel .logo-small {
    position: absolute;
    bottom: 40px;
    left: 80px;
    height: 56px;
    width: 41px;
    background: url(<%= asset_path 'new/main/logo-small.png' %>);
}

#preloader, #content-preloader {display: none;  position: relative; left: 0; top: 0; z-index: 999; width: 100%; height: 100%; overflow: hidden; background: rgba(255, 255, 255, 0.9) url('<%= asset_path 'main/loader.gif' %>') no-repeat center center; }

/***********************************************************************************************************************
/   User
/**********************************************************************************************************************/

#logo-login {
    display: block;
    width: 90%;
    max-width: 655px;
    margin: 80px auto;
}

.login-container {
    width: 340px;
    position: relative;
    display: inline-block;
    vertical-align: top;
    margin-top: 30px;
}
#sign-in.login-container,
#sign-up.login-container { margin-left: 30px; }

.login-container .title {
    font-size: 24px;
    font-weight: 400;
    margin-bottom: 20px;
}
.login-container .text-field {
    width: 335px;
    height: 57px;
    line-height: 58px;
    font-size: 20px;
    margin-bottom: 10px;
}
.login-container .login-button {
    float: right;
    padding: 15px 40px;
    font-size: 18px;
}
.login-container .signup-button {
    display: block;
    width: 335px;
    height: 61px;
    margin-bottom: 10px;
}
.login-container .linkedin-sign-up { background: url(<%= asset_path 'new/signup/linkedin-login.png' %>); }
.login-container .sign-up { background: url(<%= asset_path 'new/signup/signup.png' %>); }
.login-container .linkedin-sign-up:hover,
.login-container .sign-up:hover { background-position: 0px -61px; }

/****** Sign Up Form ******/

#sign-up-form {
    margin: 0 auto 60px auto;
    width: 400px;
    position: static;
    display: block;
    vertical-align: top;
}
#sign-up-form .green-button {
    width: 100%;
    padding: 18px 0;
    font-size: 18px;
    text-align: center;
}

.image-upload {
    float: left;
    width: 120px;
    height: 120px;
    background: url(<%= asset_path 'main/button-image-upload.png' %>);
}
.image-upload:hover {
    cursor: pointer;
}
.image-upload input {
    width: 100%;
    height: 100%;
    filter: alpha(opacity=0);
    opacity: 0;
}

#signup-provider input {
    margin-right: 15px;
}
#signup-provider {
    font-size: 1.3em;
    padding: 20px 0;
}

#sign-up-form .name, #sign-up-form .email {
    width: 270px;
    float: right;
}
#sign-up-form .email { margin-bottom: 30px; }

/****** Sign Up SP ******/

#sign-up-sp-form {
    margin: 0 auto 60px auto;
    width: 850px;
    position: static;
    display: block;
    vertical-align: top;
}

#signup-address {
    margin-top: 5px;
    margin-bottom: 30px;
}
#signup-address > div {
    width: 200px;
    float: left;
    margin-right: 10px;
}
#signup-address > div > div {
    width: 200px;
}

#sign-up-sp-form p {
    font-size: 1.4em;
    padding-bottom: 10px;
}

#sp-checkbox {
    display: inline-block;
    width: 55px;
    height: 55px;
    background: transparent url(<%= asset_path 'main/check-box.png' %>) top left no-repeat;
    background-position: 0px 0px;
    cursor: pointer;
    vertical-align: middle;
}

#signup-left {
    width: 400px;
    float: left;
}
#signup-left div {
    height: 88px;
}

#signup-right {
    width: 400px;
    float: right;
}

#signup-skills {
    width: 48%;
    float: left;
}
#signup-skills .text-field {
    width: 100%;
}

#signup-hourly-rate {
    width: 48%;
    float: right;
}

.signup-form-sp-button {
    width: 50%;
    margin: 25px auto;
    padding: 15px 0;
    font-size: 18px;
    text-align: center;
}

#signup-address > div, #signup-address > input {
    float: left;
    width: 200px;
    margin-right: 14px;
}

#search-options {
    margin-top: -12px;
    margin-bottom: 10px;
}
#search-options .search-field {
    margin-left: 10px;
    line-height: 50px;
    text-indent: 10px;
    font-size: 18px;
}
.search-option {
    display: inline-block;
    width: 174px;
    margin-left: 3px;
    position: relative;
    vertical-align: top;
}
.search-option .text-field {
    line-height: 50px;
    text-indent: 10px;
    font-size: 18px;
    width: 100%;
}
.search-option .option-container {
    width: 234px;
    position: absolute;
    top: 55px;
    left: 0px;
    background-color: hsla(0, 0%, 0%, 0.8);
    cursor: pointer;
    max-height: 400px;
    overflow: auto;
    z-index: 1000;
    color: #FFF;

}
.search-option .option-container .option {
    padding: 12px 12px;
    border-top: 1px solid #FFF;
    cursor: pointer;
}
.search-option .option-container .option:hover { background: #239060;  }
.search-option .sort-by {
    height: 55px;
    margin-top: 1px;
    vertical-align: top;
    font-size: 18px;
    width: 170px;
    background: url(<%= asset_path 'new/search/sortby.png' %>);
    outline: 0;
    display: none;
    cursor: pointer;
}
.search-option .sort-by.active { display: block; }
.search-option .sort-by.cost { background-position: 0px 55px; }
.search-option .sort-by.cost:hover,
.search-option .sort-by.cost.active { background-position: 0px 0px; }
.search-option .sort-by.experience { background-position: 170px 55px; }
.search-option .sort-by.experience:hover,
.search-option .sort-by.experience.active { background-position: 170px 0px; }
.search-option .sort-by.distance { background-position: 340px 55px; }
.search-option .sort-by.distance:hover,
.search-option .sort-by.distance.active { background-position: 340px 0px; }
#favorites { margin-top: -19px; }

#map {
    margin-top: 7px;
    height: 100%;
    width: 115%;
    background-color: #EEE;
}

#user-info {
    display: none;
    width: 150px;
    margin: 40px auto 30px auto;
    text-align: center;
}
#user-info .name,
#user-info .location {
    text-transform: uppercase;
    font-weight: 400;
    font-size: 14px;
    color: #888;
}
#user-info .name {
    font-size: 16px;
    color: #000;
}
#user-info .avatar {
    width: 75px;
    height: 75px;
    display: block;
    margin: 40px auto 30px auto;
}

/***********************************************************************************************************************
/   Messages
/**********************************************************************************************************************/

#messages { margin-top: -19px; }
#nda-sent {
    float: left;
    margin-left: 15px;
    font-size: 18px;
    margin-right: -35px;
}
#nda-sent b { font-weight: 700; }
.message-box {
    min-height: 115px;
    vertical-align: top;
    position: relative;
    border: 10px solid #FFF;
    background: #FFF;
    margin-bottom: 1px;min-height: 115px;
    vertical-align: top;
    position: relative;
    border: 10px solid #FFF;
    background: #FFF;
    margin-bottom: 1px;
    cursor: pointer;
}
.message-box .black-button {
    position: absolute;
    width: 30px;
    text-align: center;
    line-height: 30px;
    left: -10px;
    top: -10px;
    z-index: 1000;
    display: none;
}
.message-box .green-button {
    display: inline-block;
    width: 12px;
    height: 12px;
    border-radius: 6px;
    opacity: 0;
    z-index: 1000;
    margin-left: 10px;
}
.message-box .green-button.active { opacity: 1; }
.message-box .avatar-container {
    display: inline-block;
    height: 95px;
    overflow: hidden;
}
.message-box .avatar-container img {
    width: 110px;
    height: 110px;
    position: relative;
    top: -10px;
}
.message-box .name {
    text-align: center;
    padding-bottom: 20px;
    word-wrap: break-word;
    display: inline-block;
    vertical-align: top;
    margin-left: 10px;
    margin-top: 10px;
    font-weight: 700;
    font-size: 22px;
}
.message-box .content {
    position: absolute;
    top: 45px;
    left: 120px;
    font-size: 18px;
    word-break: break-word;
    line-height: 22px;
}
.message-box .time {
    position: absolute;
    top: 10px;
    right: 0px;
    text-transform: uppercase;
    color: #777;
    font-weight: 400;
    font-size: 13px;
}

#chat {
    height: 539px;
    margin-top: -19px;
    margin-bottom: 1px;
    overflow: auto;
}
#chat-area {
    background: hsla(0, 0%, 100%, 0.3);
    border: 1px solid #FFF;
    margin-left: -1px;
    padding: 10px;
}
#chat-box {
    display: inline-block;
    vertical-align: top;
    background-color: rgb(255, 255, 255);
    min-width: 580px;
    max-width: 580px;
    min-height: 48px;
    max-height: 48px;
    padding: 14px;
    font-size: 18px;
    margin: 0;
}
#post-chat {
    display: inline-block;
    width: 120px;
    text-align: center;
    line-height: 75px;
    font-size: 18px;
}

/***********************************************************************************************************************
/   Presentation
/**********************************************************************************************************************/

h1 {
    line-height: 120px;
    font-weight: 400;
    text-align: center;
    font-size: 40px;
    color: #000;
    letter-spacing: 5px;
    height: 115px;
    background: hsla(0, 0%, 100%, 0.6);
    min-width: 640px;
}

.container {
    margin: 1px;
    padding: 10px 0px;
    background-color: #FFF;
}

.logo {
    display: block;
    width: 90%;
    max-width: 655px;
    margin: auto;
    border-top: 30px solid transparent;
    margin-bottom: 30px;
}

.video-container {
    width: 640px;
    margin: auto auto 30px auto;
    background: url(<%= asset_url 'presentation/video-bg.png' %>);
    height: 360px;
    border: 2px solid #000;
    text-align: center;
    font-size: 20px;
    line-height: 70px;
    color: #FFF;
}
.video-container .play-btn {
    width: 95px;
    margin: auto;
    background: url(<%= asset_url 'presentation/play-btn.png' %>) 50% 100% no-repeat;
    height: 57%;
    cursor: pointer;
}

#newsletter-sign-up {
    font-size: 24px;
    text-align: center;
    line-height: 120px;
    margin: 1px 0px;
    background: #fff;
}
#newsletter-sign-up .field {
    padding: 2px 16px 0px 16px;
    width: 250px;
    height: 50px;
    line-height: 40px;
    font-size: 18px;
    margin: 0px 8px;
    display: inline-block;
    text-align: left;
    border: 1px solid #1f9966;
    color: #239060;
    vertical-align: middle;
}
#newsletter-sign-up #register-form-send {
    display: inline-block;
    background: #239060;
    background-size: 1px 100%;
    width: 160px;
    padding: 4px 0px;
    line-height: 46px;
    color: #FFF;
    position: relative;
    top: 2px;
}
#newsletter-sign-up #register-form-send::-webkit-input-placeholder { color: #239060 !important; }
#newsletter-sign-up #register-form-send:-moz-placeholder { color: #239060 !important; }
#newsletter-sign-up #register-form-send::-moz-placeholder { color: #239060 !important; }
#newsletter-sign-up #register-form-send:-ms-input-placeholder { color: #239060 !important; }

#about-page, #press-page, #contact-page {
    opacity: 1;
    display: block;
}

#social-twitter-container {
    margin: auto;
    width: 960px;
}

.twitter-timeline-rendered {
    display: inline-block;
    vertical-align: top;
    margin-left: 60px;
    margin-top: 60px;
    width: 520px;
    height: 255px;
}

#footer .appstore-buttons {
    margin: 0px auto 40px auto;
    width: 460px;
}
#footer .appstore-buttons .ios,
#footer .appstore-buttons .android { display: inline-block; }
#footer .social { margin: 40px auto; }
#footer .copy {
    text-align: center;
    margin-bottom: 40px;
}

#presentation-header .appstore-buttons {
    display: inline-block;
    font-size: 20px;
