 * compiled file, but it's generally better to create a new file per style scope.
 *
 *= require_self
 *= require_tree .
 */

/***********************************************************************************************************************
/   Main
/**********************************************************************************************************************/

@font-face {
    font-family: 'main';
    font-weight: 100;
    src: url('<%= asset_url "light.otf" %>');
}
@font-face {
    font-family: 'main';
    font-weight: 400;
    src: url('<%= asset_url "regular.otf" %>');
}
@font-face {
    font-family: 'main';
    font-weight: 900;
    src: url('<%= asset_url "bold.otf" %>');
}

html, body, div, span, applet, object, iframe, h1, h2, h3, h4, h5, h6, p, blockquote, pre, a, abbr, acronym, address, big, cite, code, del, dfn, em, img, ins, kbd, q, s, samp, small, strike, strong, sub, sup, tt, var, b, u, i, center, dl, dt, dd, ol, ul, li, fieldset, form, label, legend, table, caption, tbody, tfoot, thead, tr, th, td, article, aside, canvas, details, embed, figure, figcaption, footer, header, hgroup, menu, nav, output, ruby, section, summary, time, mark, audio, video {
    margin: 0;
    padding: 0;
    border: 0;
    font-size: 100%;
    font: inherit;
    vertical-align: baseline;
    -moz-box-sizing: border-box;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
}

html:before, body:before, div:before, span:before, applet:before, object:before, iframe:before, h1:before, h2:before, h3:before, h4:before, h5:before, h6:before, p:before, blockquote:before, pre:before, a:before, abbr:before, acronym:before, address:before, big:before, cite:before, code:before, del:before, dfn:before, em:before, img:before, ins:before, kbd:before, q:before, s:before, samp:before, small:before, strike:before, strong:before, sub:before, sup:before, tt:before, var:before, b:before, u:before, i:before, center:before, dl:before, dt:before, dd:before, ol:before, ul:before, li:before, fieldset:before, form:before, label:before, legend:before, table:before, caption:before, tbody:before, tfoot:before, thead:before, tr:before, th:before, td:before, article:before, aside:before, canvas:before, details:before, embed:before, figure:before, figcaption:before, footer:before, header:before, hgroup:before, menu:before, nav:before, output:before, ruby:before, section:before, summary:before, time:before, mark:before, audio:before, video:before, html:after, body:after, div:after, span:after, applet:after, object:after, iframe:after, h1:after, h2:after, h3:after, h4:after, h5:after, h6:after, p:after, blockquote:after, pre:after, a:after, abbr:after, acronym:after, address:after, big:after, cite:after, code:after, del:after, dfn:after, em:after, img:after, ins:after, kbd:after, q:after, s:after, samp:after, small:after, strike:after, strong:after, sub:after, sup:after, tt:after, var:after, b:after, u:after, i:after, center:after, dl:after, dt:after, dd:after, ol:after, ul:after, li:after, fieldset:after, form:after, label:after, legend:after, table:after, caption:after, tbody:after, tfoot:after, thead:after, tr:after, th:after, td:after, article:after, aside:after, canvas:after, details:after, embed:after, figure:after, figcaption:after, footer:after, header:after, hgroup:after, menu:after, nav:after, output:after, ruby:after, section:after, summary:after {
    -moz-box-sizing: border-box;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
}

article, aside, details, figcaption, figure, footer, header, hgroup, menu, nav, section { display: block; }
ol, ul { list-style: none; }
blockquote, q { quotes: none; }
blockquote:before, blockquote:after, q:before, q:after { content: none; }

input, textarea {
    outline: none;
    -moz-appearance: none;
    -webkit-appearance: none;
    appearance: none;
    border: 0;
    font-family: 'main', Helvetica, sans-serif;
    text-transform: uppercase;
}
::-webkit-input-placeholder { color: #000; }
:-moz-placeholder { color: #000; }
::-moz-placeholder { color: #000; }
:-ms-input-placeholder { color: #000; }


table {
    border-collapse: collapse;
    border-spacing: 0;
}

a { cursor: pointer; }

body {
    line-height: 1;
    background: url(<%= asset_path "new/main/background.png" %>);
    color: #000;
    font-family: 'main', Helvetica, sans-serif;
    font-weight: 100;
    font-size: 14px;
    min-width: 660px;
}

a {
    text-decoration: none;
    cursor: pointer;
    color: #1b915f;
}
a:hover { color: #000; }

.t {
    -webkit-transition: all 0.5s;
    -moz-transition: all 0.5s;
    -o-transition: all 0.5s;
    -ms-transition: all 0.5s;
    -webkit-transition: all 0.5s;
}

#title {
    text-align: center;
    line-height: 60px;
    font-size: 24px;
    font-weight: bold;
    margin-bottom: 20px;
}
#title .black-button {
    float: right;
    font-size: 16px;
    height: 40px;
    line-height: 43px;
    padding: 0px 30px;
    font-weight: 100;
    margin-top: 10px;
}
#title .green-button {
    font-size: 16px;
    height: 40px;
    line-height: 43px;
    padding: 0px 20px;
    margin-top: 10px;
    font-weight: 100;
    float:left;
    opacity: 0;
}
#title .green-button.active { opacity: 1; }

.text-field {
    display: block;
    text-indent: 20px;
    background-color: #FFF;
    box-shadow: 0px 0px 10px 0px #CCC;
    outline: 0;
}
.text-field.pass-field { font-family: 'password'; }

.lt-drop {
    width: 102px;
    height: 23px;
    background: url(<%= asset_path "main/drops.png" %>);
}
.lt-drop.hidden { display: none; }
.lt-drop.blue { background-position: 102px 0px; }
.lt-drop.bronze { background-position: 102px 23px; }
.lt-drop.silver { background-position: 102px 46px; }
.lt-drop.gold { background-position: 102px 69px; }

.star {
    vertical-align: top;
    display: inline-block;
    width: 15px;
    height: 14px;
    background: url(<%= asset_path "main/stars.png" %>);
}
.star.active { background-position: 14px 0px; }

.favorite-button { background: url(<%= asset_path "main/button-favorite-small.png" %>); }
.play-button { background: url(<%= asset_path "main/button-play.png" %>); }
.favorite-button,
.play-button {
    background-position: 0px 0px;
    cursor: pointer;
}
.favorite-button:hover,
.play-button:hover { background-position: 0px -50px; }
.favorite-button.active,
.play-button.disabled { background-position: 50px 0px; }
.favorite-button.active:hover,
.play-button.disabled:hover { background-position: 50px -50px; }

.avatar {
    display: block;
    background: #000;
    cursor: pointer;
    border-radius: 0px;
}

.green-button {
    display: block;
    background: #1b915f;
    background-size: 1px 100%;
    color: #FFF;
}
.green-button:hover, .green-button:focus {
    background: #000 !important;
    color: #FFF;
}

.black-button {
    display: block;
    background: #000;
    background-size: 1px 100%;
    color: #FFF;
}
.black-button:hover, .black-button:focus {
    background: #1b915f !important;
    color: #FFF;
}

.notification {
    position: absolute;
    right: 10px;
    top: 10px;
    color: #FFF;
    height: 0px;
    line-height: 23px;
    padding: 0px 5px 20px 5px;
    border-radius: 10px;
    opacity: 0;
}
.notification.active { opacity: 1; }

#wrapper {
    height: 700px;
    width: 960px;
    margin: 0px auto;
    position: relative;
    overflow: hidden;
}
#background {
    position: absolute;
    height: 680px;
    width: 740px;
    top: 10px;
    right: 10px;
    overflow: hidden;
}
#background.disabled {
    top: 0%;
    right:0%;
    width: 100%;
    height: 100%;
}
#admin-panel {
    position: relative;
    z-index: 1;
    width: 210px;
    height: 710px;
    background-color: hsla(0, 0%, 100%, 0.3);
    border: 1px solid #FFF;
    bottom: 10px;
}
#admin-panel.disabled { display: none; }
#admin-panel .header {
    padding: 10px;
    height: 180px;
    position: relative;
}

#admin-panel .header .login {
    width: 210px;
    left: 0px;
    bottom: 34%;
    position: absolute;
    text-align: center;
    padding: 18px;
    font-size: 20px;
}
#admin-panel .navigation .link {
    display: block;
    width: 190px;
    font-size: 20px;
    line-height: 40px;
    text-indent: 25px;
    margin-bottom: 20px;
    word-spacing: 10px;
    color: #000;
}
#admin-panel .navigation .link:hover { color: #1b915f; }
#admin-panel .navigation .link .icon {
    display: inline-block;
    vertical-align: middle;
    width: 29px;
    height: 27px;
    background: url(<%= asset_url 'new/main/icons-navigation.png' %>) repeat-y;
}
#admin-panel .navigation .settings.link .icon.hover,
#admin-panel .navigation .favorites.link .icon.hover,
#admin-panel .navigation .messages.link .icon.hover {
    width: 28px;
    margin-left: 1px;
}
#admin-panel .navigation .search.link .icon.hover,
#admin-panel .navigation .projects.link .icon.hover {
    width: 27px;
    margin-left: 2px;
}
#admin-panel .navigation .search.link .icon { background-position: 0px 0px; }
#admin-panel .navigation .search.link .icon.hover { background-position: -29px 0px; }

#admin-panel .navigation .projects.link .icon {
    height: 25px;
    background-position: 0px -54px;
}
#admin-panel .navigation .projects.link .icon.hover { background-position: -29px -54px; }

#admin-panel .navigation .messages.link .icon { background-position: 0px -79px; }
#admin-panel .navigation .messages.link .icon.hover { background-position: -29px -79px; }

#admin-panel .navigation .favorites.link .icon { background-position: 0px -106px; }
#admin-panel .navigation .favorites.link .icon.hover { background-position: -29px -106px; }

#admin-panel .navigation .stats.link .icon {
    height: 26px;
    background-position: 0px -133px;
}
#admin-panel .navigation .stats.link .icon.hover { background-position: -29px -133px; }

#admin-panel .navigation .settings.link .icon {
    height: 29px;
    background-position: 0px -159px;
}
#admin-panel .navigation .settings.link .icon.hover { background-position: -29px -159px; }

#admin-panel .notification { background-color: #1b915f; }

#admin-panel .logo-small {
    position: absolute;
    bottom: 40px;
    left: 80px;
    height: 56px;
    width: 41px;
    background: url(<%= asset_path "new/main/logo-small.png" %>);
}

#preloader, #content-preloader {display: none;  position: relative; left: 0; top: 0; z-index: 999; width: 100%; height: 100%; overflow: hidden; background: rgba(255, 255, 255, 0.9) url('/assets/main/loader.gif') no-repeat center center; }

/***********************************************************************************************************************
/   User
/**********************************************************************************************************************/

#logo-login {
    display: block;
    width: 90%;
    max-width: 655px;
    margin: 80px auto;
}

.login-container {
    width: 400px;
    position: relative;
    display: inline-block;
    vertical-align: top;
    margin-top: 100px;
}
#sign-in.login-container { margin-left: 65px; }
#sign-up.login-container { margin-left: 30px; }

.login-container .title {
    font-size: 24px;
    font-weight: bold;
    text-align: center;
    margin-bottom: 30px;
}
.login-container .text-field {
    width: 400px;
    height: 55px;
    line-height: 58px;
    font-size: 20px;
    margin-bottom: 10px;
}
.login-container .login-button {
    float: right;
    padding: 15px 40px;
    font-size: 18px;
}
.login-container .signup-button {
    display: block;
    width: 399px;
    height: 55px;
    background: url(<%= asset_path "sign-up/buttons-linkedin-signup.png" %>);
    margin-bottom: 10px;
}
.login-container .linkedin-sign-up { background-position: 0px 0px; }
.login-container .linkedin-sign-up:hover { background-position: 0px -55px; }
.login-container .sign-up { background-position: 399px 0px; }
.login-container .sign-up:hover { background-position: 399px -55px; }


#logo-header {
    position: relative;
    background: rgba(255, 255, 255, 0.6);
    box-shadow: 0px 5px 10px 0px #CCC;
}
#logo-header #logo-login {
    margin: 0 auto 40px auto;
    padding: 18px 0;
    width: 493px;
}
#logo-header .black-button {
    padding: 12px 30px;
    position: absolute;
    right: 0;
    top: 20px;
    font-size: 18px;
}

/****** Sign Up Form ******/

#sign-up-form {
    margin: 0 auto 60px auto;
    width: 400px;
    position: static;
    display: block;
    vertical-align: top;
}
#sign-up-form .green-button {
    width: 100%;
    padding: 18px 0;
    font-size: 18px;
    text-align: center;
}

.image-upload {
    float: left;
    width: 120px;
    height: 120px;
    background: url(<%= asset_path "main/button-image-upload.png" %>);
}
.image-upload:hover {
    cursor: pointer;
}
.image-upload input {
    width: 100%;
    height: 100%;
    filter: alpha(opacity=0);
    opacity: 0;
}

#signup-provider input {
    margin-right: 15px;
}
#signup-provider {
    font-size: 1.3em;
    padding: 20px 0;
}

#sign-up-form .name, #sign-up-form .email {
    width: 270px;
    float: right;
}
#sign-up-form .email { margin-bottom: 30px; }

/****** Sign Up SP ******/

#sign-up-sp-form {
    margin: 0 auto 60px auto;
    width: 850px;
    position: static;
    display: block;
    vertical-align: top;
}

#signup-address {
    margin-top: 5px;
    margin-bottom: 30px;
}
#signup-address > div {
    width: 200px;
    float: left;
    margin-right: 10px;
}
#signup-address > div > div {
    width: 200px;
}

#sign-up-sp-form p {
    font-size: 1.4em;
    padding-bottom: 10px;
}

#sp-checkbox {
    display: inline-block;
    width: 55px;
    height: 55px;
    background: transparent url(<%= asset_path "main/check-box.png" %>) top left no-repeat;
    background-position: 0px 0px;
    cursor: pointer;
    vertical-align: middle;
}

#signup-left {
    width: 400px;
    float: left;
}
#signup-left div {
    height: 88px;
}

#signup-right {
    width: 400px;
    float: right;
}

#signup-skills {
    width: 48%;
    float: left;
}
#signup-skills .text-field {
    width: 100%;
}

#signup-hourly-rate {
    width: 48%;
    float: right;
}

.signup-form-sp-button {
    width: 50%;
    margin: 25px auto;
    padding: 15px 0;
    font-size: 18px;
    text-align: center;
}

#signup-address > div, #signup-address > input {
    float: left;
    width: 200px;
    margin-right: 14px;
}

#cities, #skills, #schools, #states, #categories, #subcategories {
    width: 234px;
    position: absolute;
    margin-top: -10px;
    box-shadow: 0px 0px 10px 0px #CCC;
    display: none;
    cursor: pointer;
    max-height: 400px;
    overflow: auto;
}

#cities div,
#skills div,
#schools div,
#states div,
#categories div,
#subcategories div {
    background: #fff;
    padding: 12px 12px;
    border-top: 1px solid #ccc;
    cursor: pointer;
    margin-top: -1px;
}

#cities div:hover,
#skills div:hover,
#schools div:hover,
#states div:hover,
#categories div:hover,
#subcategories div:hover { background: #f6f6f6; }

#filter {
    display: none;
    position: absolute;
    left: 0 ;
    top: 0;
    z-index: 999;
    width: 100%;
    height: 100%;
    overflow: hidden;
    background: rgba(0, 0, 0, 0.5);
}
#filter .inner {
    position: absolute;
    width: 540px;
    right: -540px;
    top: 0;
}
#filter .toggle {
    width: 40px;
    float: left;
    background: none;
}
#filter .toggle .visibility-toggle {
    margin: 0;
    z-index: 1000;
}
#filter .green-button {
    margin-top: 20px;
    padding: 11px 10px 9px 10px;
    text-align: center;
    font-size: 20px;
}
#filter #rate, #hourly-rate { margin-bottom: 26px; }

#filter .content {
    width: 500px;
    float: left;
    margin: 0;
    background: #fff;
    padding: 10px;
    height: 213px;
}
#filter .content .options .text-field {
    text-indent: 10px;
    width: 230px;
    font-size: 16px;
    margin-bottom: 10px;
    line-height: 38px;
}
#filter .content .options,
#filter .content .sliders {
    width: 230px;
    display: inline-block;
    vertical-align: top;
}
#filter .content .sliders,
#filter .content .sliders .slider { margin-left: 10px; }
#filter .content .sliders .title {
    font-size: 20px;
    margin: 5px 0 15px 0;
}

#search-tool-bar {
    box-shadow: 0px 0px 10px 0px #CCC;
    background: #FFF;
}
#search-tool-bar .title {
    font-size: 24px;
    font-weight: 600;
    text-align: center;
    line-height: 60px;
}
#search-tool-bar .functions {
    position: absolute;
    right: 10px;
    top: 10px;
}
.functions .geo-toggle {
    display: inline-block;
    height: 40px;
    line-height: 45px;
    padding: 0px 20px;
    font-size: 20px;
    vertical-align: top;
}
.functions .visibility-toggle {
    display: inline-block;
    vertical-align: top;
    width: 40px;
    height: 40px;
    background-color: #000;
    margin-left: 10px;
    background: url(<%= asset_path "search/visibility-toggle.png" %>);
    background-position: 0px 40px;
    cursor: pointer;
}
.functions .visibility-toggle:hover { background-position: 40px 40px; }
.functions .visibility-toggle.active { background-position: 0px 0px; }
.functions .visibility-toggle.active:hover { background-position: 40px 0px; }
#map {
    height: 100%;
    width:115%;
    background-color: #EEE;
}

#user-info {
    display: none;
    background: #fff;
    position: absolute;
    bottom: 15px;
    width: 100%;
}
#user-info > div {
    padding: 10px 10px 10px 0;
    line-height: 1.5;
}

#user-info img {
    float: left;
    margin-right: 10px;
}

#user-drop-down {
    margin-top: 15px;
    margin-left: 10px;
}

#user-menu {
    display: none;
    width: 170px;
    position: absolute;
    margin: 0;
    z-index: 999999;
}
#user-menu a {
    display: block;
    padding: 12px 0;
    text-align: center;
    background: #fff;
    margin-bottom: 1px;
}
#account-btn {
    position: relative;
    display: block;
    margin: 0;
    padding: 6px 0 0 27px;
    color: #fff;
    font-size: 20px;
    height: 40px;
    background: url(/assets/main/gradient-green.png);
}

#account-btn div {
    position: absolute;
    right: 0;
    top: 0;
    width: 40px;
    height: 40px;
    background: url(<%= asset_path 'admin-panel/button-drop-down.jpg' %>);
    background-position: 0px 0px;
}

#account-btn:hover div {
    background-position: 0px -40px;
}

/***********************************************************************************************************************
/   Messages
/**********************************************************************************************************************/

.message-user {
    width: 150px;
    display: inline-block;
    vertical-align: top;
    margin-left: 20px;
    position: relative;
    box-shadow: 0px 0px 10px 0px #CCC;
}
.message-user .green-button {
    position: absolute;
    width: 30px;
    text-align: center;
    line-height: 30px;
    right: 0px;
    border-left: 1px solid #FFF;
    border-bottom: 1px solid #FFF;
    opacity: 0;
    z-index: 1000;
}
.message-user .black-button {
    position: absolute;
    width: 30px;
    text-align: center;
    line-height: 30px;
    right: 0px;
    border-left: 1px solid #FFF;
    border-bottom: 1px solid #FFF;
    opacity: 0;
    z-index: 1000;
}
.message-user .green-button.active,
.message-user .black-button.active { opacity: 1; }
.message-user .avatar {
    width: 125px;
    margin: auto;
    display: block;
    margin-bottom: 20px;
}
.message-user .name {
    text-align: center;
    padding-bottom: 20px;
    word-wrap: break-word;
}

#chat {
    height: 330px;
    margin-bottom: 30px;
    overflow: scroll
}
#chat .chat-box {
    box-shadow: 0px 0px 10px 0px #CCC;
    background: #FFF;
    margin-top: 10px;
    width: 680px;
    margin: 10px auto 0px auto;
    text-indent: 50px;
}
#chat .chat-box .title {
    position: relative;
    background: #CCC;
    font-size: 18px;
    color: #FFF;
}
#chat .chat-box .title .avatar {
    position: absolute;
    width: 50px;
    height: 50px;
}
#chat .chat-box .title .name {
    color: #FFF;
    padding: 8px;
}
#chat .chat-box .title .date {
    padding: 8px;
    float:right;
}
#chat .chat-box .chat { padding: 8px; }
#chat-box {
    display: block;
    background-color: rgb(255, 255, 255);
    max-width: 640px;
    max-height: 100px;
    width: 100%;
    height: 100%;
    margin: 0px auto 30px auto;
    box-shadow: rgb(204, 204, 204) 0px 0px 10px 0px;
    padding: 20px;
    font-size: 18px;
}
#create-chat {
    margin: auto;
    width: 120px;
    text-align: center;
    line-height: 44px;
    font-size: 18px;
    height: 40px;
}

/***********************************************************************************************************************
/   Presentation
/**********************************************************************************************************************/

h1 {
    line-height: 120px;
    font-weight: 400;
    text-align: center;
    font-size: 40px;
    color: #000;
    letter-spacing: 5px;
    height: 115px;
    background: hsla(0, 0%, 100%, 0.6);
    min-width: 640px;
}

.container {
    margin: 1px;
    padding: 10px 0px;
    background-color: #FFF;
}

.logo {
    display: block;
    width: 90%;
    max-width: 655px;
