<div id="search-options" title="<%= listing %>">

  <input type="text" placeholder="Search" class="text-field search-option search-field" onkeyup="setQuery(this.value);">

  <div class="search-option" style="margin-left: 5px;">
    <input type="text" placeholder="Industry" class="text-field category">
    <div class="option-container" id="industries" style="display: none;"></div>
  </div>

  <div class="search-option" style="margin-left: 10px;">
    <input type="text" placeholder="College" class="text-field school" onkeyup="searchSchool(this.value);">
    <div class="option-container" id="schools"></div>
  </div>
  <div class="search-option" style="margin-left: 13px;">
    <div class="sort-by cost active" title="cost"></div>
    <!--div class="sort-by distance" title="distance"></div-->
    <div class="sort-by experience" title="experience"></div>
  </div>

  <input type="hidden" name="" id="query">
  <input type="hidden" name="" id="category_id">
  <input type="hidden" name="" id="school_id">

</div>